{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ],
    "build": {
        "env": {
            "PRISMA_GENERATE_COMMAND": "npm run vercel-build",
            "PRISMA_CLIENT_OUTPUT": "./generated/prisma"
        }
    }
}